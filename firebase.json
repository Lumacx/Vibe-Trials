{
  "hosting": {
    "site": "retrovibe-arcade",
    "source": ".",
    "frameworksBackend": {
      "codebase": "default"
    },
    "headers": [
      {
        "source": "/**",
        "headers": [
          {
            "key": "Content-Security-Policy",
            "value": "default-src 'self'; script-src 'self' 'unsafe-eval'; style-src 'self' 'unsafe-inline'; object-src 'none';"
          }
        ]
      }
    ]
  }
}
